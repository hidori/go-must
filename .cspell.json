{
  "language": "en",
  "ignorePaths": [".cspell.json", ".golangci.yml"],
  "words": ["contexttime", "golangci", "hidori", "Println"],
  "languageSettings": [
    {
      "languageId": "go",
      "allowCompoundWords": false,
      "includeRegExpList": ["CStyleComment", "string"],
      "ignoreRegExpList": [
        "import\\s*\\((.|[\r\n])*?\\)",
        "import\\s*.*\".*?\"",
        "//\\s*go:generate.*",
        "//\\s*nolint:.*",
        "//\\s*cSpell:.*"
      ]
    }
  ]
}
